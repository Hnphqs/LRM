  gg.searchNumber("12004", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
  gg.getResults(100)
  gg.editAll("13514", gg.TYPE_DWORD)
  gg.toast("ä¿®æ”¹æˆåŠŸ")
  gg.clearResults()
end


